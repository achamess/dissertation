  year = {2013},
  keywords = {An{\'a}lisis de riesgo,commercial shipping,Generalized additive models,Habitat modeling,modelado del h{\'a}bitat,modelos aditivos generalizados,navegaci{\'o}n comercial,Risk analysis},
  pages = {292--302},
