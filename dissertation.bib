  month = nov,
  year = {2012},
  pages = {e47598},
  file = {Kim et al - 2012 - Catching the Right Wave.pdf:/Users/bbest/zotero/storage/WPX2NF6J/Kim et al - 2012 - Catching the Right Wave.pdf:application/pdf}
