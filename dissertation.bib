  author = {Liu, Jianguo and Mooney, Harold and Hull, Vanessa and Davis, Steven J. and Gaskell, Joanne and Hertel, Thomas and Lubchenco, Jane and Seto, Karen C. and Gleick, Peter and Kremen, Claire and Li, Shuxin},
  month = feb,
